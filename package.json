{
  "name": "3d-portfolio",
  "homepage": "https://samratbaral.github.io/3d-portfolio/",
  "private": false,
  "version": "1.0.1",
  "type": "module",
  "description": "Personal Website Version 1.0.1",
  "author": "Samrat Baral <samratbaral.sb@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/samratbaral/samratbaral.github.io"
  },
  "keywords": [
    "three.js",
    "react",
    "portfolio",
    "personal website",
    "3d"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "predeploy": "vite build",
    "deploy": "gh-pages -d dist",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "lint-staged": "lint-staged",
    "all": "concurrently --kill others \"npm run format\"  \"npm run deploy\"  \"npm run dev\" "
  },
  "lint-staged": {
    "*.{js,css,json,md}": [
      "prettier --write"
    ],
    "*.js": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@emailjs/browser": "^3.11.0",
    "@react-three/drei": "^9.79.4",
    "@react-three/fiber": "^8.13.5",
    "framer-motion": "^10.13.1",
    "maath": "^0.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.2",
    "react-tilt": "^1.0.2",
    "react-vertical-timeline-component": "^3.6.0",
    "three": "^0.154.0",
    "vite": "^4.4.11"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.2.1",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "gh-pages": "^6.0.0",
    "lint-staged": "^15.0.1",
    "postcss": "^8.4.27",
    "prettier": "^3.0.3",
    "tailwindcss": "^3.3.3"
  }
}
